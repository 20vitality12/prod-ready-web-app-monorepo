{
	"name": "monolith",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"prepare": "husky install",
		"format:all": "nx format:write --all",
		"build:all": "nx run-many --target=build --all",
		"build:affected": "nx affected --target=build --output-style=stream",
		"test:all": "nx run-many --target=test --all --output-style=stream --skip-nx-cache",
		"test:affected": "nx affected --target=test --output-style=stream",
		"prisma:generate": "prisma generate",
		"prisma:migrate:create": "prisma migrate dev --create-only",
		"docker:compose:up:dev": "docker-compose -f ./docker-compose.yml up -d --build",
		"docker:compose:down:dev": "docker-compose -f ./docker-compose.yml down --remove-orphans",
		"serve:server-main": "nx generate:client prisma && npm run docker:compose:up:dev && nx migrate:deploy prisma && nx seed prisma && nx serve server"
	},
	"private": true,
	"prisma": {
		"schema": "libs/prisma/src/schema.prisma",
		"seed": "ts-node libs/prisma/src/seed.ts"
	},
	"devDependencies": {
		"@codebrew/nx-aws-cdk": "^1.0.3",
