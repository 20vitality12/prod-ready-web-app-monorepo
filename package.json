		"@nrwl/nest": "^15.9.2",
		"@nrwl/node": "15.9.2",
		"@nrwl/nx-cloud": "latest",
		"@nrwl/react": "^15.9.2",
		"@nrwl/vite": "^15.9.2",
		"@nrwl/webpack": "15.9.2",
		"@nrwl/workspace": "15.9.2",
		"@ns3/nx-serverless": "*",
		"@testing-library/react": "14.0.0",
